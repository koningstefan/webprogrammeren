<head>
<title>Puzzel 4</title>
<script type="text/javascript">
	function vergelijkWachtwoord(){
		var wachtwoord1 = prompt("Wat is wachtwoord 1?");
		var wachtwoord2 = prompt("Wat is wachtwoord 1?");

		if(wachtwoord1 === wachtwoord2){
			alert("Wachtwoorden komen overeen");
		}
		else{
			alert("YOU FAILED");
		}
		


	};


</script>

</head>s

<body>
	<h1>Puzzel 4</h1>
	<button onclick="vergelijkWachtwoord()">Wachtwoord</button>
</body>

</html> 